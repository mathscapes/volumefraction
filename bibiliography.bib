doi = {10.1016/S0734-743X(01)00040-9},
file = {:C$\backslash$:/Library/Papers/Impact Study/Papers on Experimental Setup/2002 - Belingardi, Vadori - Low velocity impact tests of laminate glass-fiber-epoxy matrix composite material plates.pdf:pdf},
