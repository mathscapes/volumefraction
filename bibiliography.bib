journal = {Compos. Sci. Technol.},
pages = {68--79},
publisher = {Elsevier Ltd},
title = {{Effect of fibre-matrix interfacial strength on the explosive blast resistance of carbon fibre laminates}},
url = {http://dx.doi.org/10.1016/j.compscitech.2016.11.009},
volume = {138},
year = {2017}
}
