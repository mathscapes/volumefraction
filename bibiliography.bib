journal = {Compos. Struct.},
keywords = {Composite,High temperature exposure,Impact behavior,Sandwich panel},
number = {1},
pages = {670--681},
title = {{Experimental study on the low velocity impact responses of all-composite pyramidal truss core sandwich panel after high temperature exposure}},
