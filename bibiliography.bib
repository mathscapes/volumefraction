isbn = {0894-1777},
issn = {08941777},
journal = {Exp. Therm. Fluid Sci.},
keywords = {Coanda jet,Recirculation,Swirl nozzles,Tufts},
number = {4},
pages = {675--682},
publisher = {Elsevier Inc.},
title = {{Formation of Coanda jet from sharp-edged swirl nozzle with base plate}},
