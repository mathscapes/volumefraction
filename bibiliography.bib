year = {2004}
}
@article{Mili2001,
abstract = {The behavior of different E-glass/epoxy laminated composite plates has been experimentally studied under impact of aluminum projectile at low velocities (0.53-3.1 m/s). The results were obtained using a drop weight impact machine and presented for three different cross-ply laminates [02/906/02], [03/904/03] and [04/902/04]. The time history of the impact process such as the acceleration impactor, the projectile displacement and the target circular plate deflection due to an impact force acting at the center has been measured. The effects of the projectile velocities and lamination sequences on composite plates behavior have been discussed. Applications of the theoretical model based on Hertzian contact law were used as an efficient guide for the experimental results validation. {\textcopyright} 2001 Elsevier Science Ltd. All rights reserved.},
