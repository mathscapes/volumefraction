@article{Wang2016a,
abstract = {The finite element analysis (FEA) is a numerical method widely used to predict$\backslash$r$\backslash$nthe metal-cutting performance in both academic and industrial studies, avoiding the$\backslash$r$\backslash$nhigh expense and time consumption of experimental methods. The problem is how to$\backslash$r$\backslash$ncalibrate reliable fracture-parameters as chip-separation criterion are implemented into$\backslash$r$\backslash$nFEA modelling. This thesis introduces a calibration method of the Johnson-Cook$\backslash$r$\backslash$nfracture parameters used in the orthogonal metal cutting modelling with a positive rake$\backslash$r$\backslash$nangle for AISI 1045 steel. These fracture parameters were obtained based on a set of$\backslash$r$\backslash$nquasi-static tensile tests, with smooth and pre-notched round bars at room temperature$\backslash$r$\backslash$nand elevated temperatures. The fracture parameters were validated by low- and highstrain$\backslash$r$\backslash$nrate simulations corresponding to tensile tests and orthogonal metal-cutting$\backslash$r$\backslash$nprocesses respectively in ABAQUS/Explicit. Compared to literature calibration$\backslash$r$\backslash$nmethods, this method is simpler, less expensive but valid.},
