@article{Khashaba2017a,
abstract = {Low-velocity impacts are unavoidable during the assembly stages and service life of aircrafts, marine and automobiles composite structures. This work aims to study the damage resistance of woven carbon fiber reinforced epoxy (CFRE) composite laminates subjected to a wide range of low-velocity impacts (15–85 J). The composite laminates were fabricated using prepreg technique. The impact tests were carried out using falling weight impact tester at room temperature (RT), 50 °C and 75 °C. The selected test temperature of 50 °C represents the summer midday temperature of some countries, while the highest temperature value (75 °C) was used to study the possible effects of unexpected temperature rise. Compression-after-impact (CAI) were undertaken to measure their residual strength. Specimens that were tested at low impact energies (15 and 35 J) as well as at higher temperature level (75 °C) are not penetrated by the impactor. Therefore, part of the kinetic energy is absorbed by the specimen in the form of damages. A second part is stored in the specimen as elastic energy, which is restituted back to the impactor for its rebound. Though no penetration is observed, the highest reduction in the stiffness and strength is observed at 75 °C. This is explained in terms of the softening and plastification of the epoxy matrix, the debonding at the interfacial fiber/matrix and the increase in the interfacial stress concentration of the re-solidified matrix. The threshold energy to initiate damage in the composite plates is measured/estimated using four methods and ranges between 2.8 and 9.6 J.},
author = {Khashaba, U. A. and Othman, Ramzi},
doi = {10.1016/j.ijimpeng.2017.04.023},
file = {:C$\backslash$:/Library/Papers/Impact Study/2017-18/2017 - Khashaba, Othman - Low-velocity impact of woven CFRE composites under different temperature levels.pdf:pdf},
issn = {0734743X},
journal = {Int. J. Impact Eng.},
pages = {191--204},
publisher = {Elsevier Ltd},
title = {{Low-velocity impact of woven CFRE composites under different temperature levels}},
volume = {108},
year = {2017}
}
