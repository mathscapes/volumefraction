journal = {Eng. Fract. Mech.},
keywords = {Adaptive remeshing technique,Crack shape evolution,Fatigue crack growth,Fatigue crack growth rate models,Fatigue crack paths,Finite element method,Stress intensity factor},
pages = {170--195},
publisher = {Elsevier Ltd},
title = {{A review on 3D-FE adaptive remeshing techniques for crack growth modelling}},
