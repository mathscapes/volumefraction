journal = {Compos. Struct.},
keywords = {Foam reinforcement,Lattice structures,Pyramidal cores},
number = {1},
pages = {696--710},
publisher = {Elsevier Ltd},
title = {{Hybrid core carbon fiber composite sandwich panels: Fabrication and mechanical response}},
url = {http://dx.doi.org/10.1016/j.compstruct.2013.10.002},
volume = {108},
year = {2014}
}
