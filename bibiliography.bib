file = {:C$\backslash$:/Library/Papers/Impact Study/Papers Cited by AK Bandaru/The effect of hybridization on the ballistic impact behavior of hybrid.pdf:pdf},
isbn = {1085300609},
issn = {13598368},
journal = {Compos. Part B Eng.},
keywords = {B. Delamination,B. Hybridization,B. Impact behaviour,C. Finite element analysis (FEA),Kevlar fiber},
pages = {300--319},
publisher = {Elsevier Ltd},
title = {{The effect of hybridization on the ballistic impact behavior of hybrid composite armors}},
