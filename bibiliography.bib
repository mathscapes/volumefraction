doi = {10.1080/17445302.2015.1131005},
file = {:C$\backslash$:/Library/Papers/Impact Study/2017-18/2017 - Abdel-Nasser, Elhewy, Al-Mallah - Impact analysis of composite laminate using finite element method.pdf:pdf},
issn = {17445302},
