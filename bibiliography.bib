keywords = {Impulsive loading,Liquid explosive,Near-field blast,Protective design},
pages = {78--89},
publisher = {Elsevier Ltd},
title = {{Experimental investigation and simplified modeling of response of steel plates subjected to close-in blast loading from spherical liquid explosive charges}},
