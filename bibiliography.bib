doi = {10.1016/j.ijimpeng.2012.11.007},
file = {:C$\backslash$:/Library/Papers/Research Papers Cited in paper/Research Papers Cited in paper/2013{\_}Iqbal MA{\_}Experimental and numerical studies of double-nosed projectile impact on aluminum plates.pdf:pdf},
isbn = {0734743X},
