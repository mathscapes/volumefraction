isbn = {9780080531571},
issn = {02663538},
journal = {Fail. Criteria Fibre-Reinforced-Polymer Compos.},
keywords = {composite laminates,failure,failure envelopes,model,predicted,strain curves,stress,theoretical},
number = {97},
pages = {379--401},
title = {{The strength of multilayered composites under a plane-stress state}},
volume = {3538},
year = {2004}
}
