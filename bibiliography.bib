title = {{Impact behavior of glass/epoxy laminated composite plates at high temperatures}},
volume = {44},
year = {2010}
}
@article{Cetin2016,
abstract = {Large-eddy simulations (LES) of turbulent hot jets emanating from realistic helicopter engine nozzle configurations at a Reynolds number of Re = 7.5 × 105and a Mach number of M=0.341 are conducted. The numerical method is based on hierarchically refined Cartesian meshes. The nozzle wall boundaries are resolved by a conservative cut-cell method. Three nozzle geometries of increasing complexity are considered, i.e., the flow fields of a clean geometry without any built-in components, a nozzle with a centerbody, and a nozzle with a centerbody plus struts are computed. The numerical method is validated by solutions for a single, a coaxial, and a chevron nozzle jet problem. A grid convergence study shows that the essential flow characteristics due to the intricacy of the nozzle geometry are well resolved. The results evidence that the flow field in the region 35 nozzle radii downstream of the exit is dominated by flow structures induced by the geometry. Compared to the clean geometry, the other two configurations show enhanced turbulent mixing. The centerbody and centerbody-plus-strut nozzle configurations reveal a spectral peak in the near nozzle exit region at St=0.15 which is caused by the wake flow of the centerbody.},
author = {Cetin, Mehmet Onur and Pauz, Vitali and Meinke, Matthias and Schr{\"{o}}der, Wolfgang},
doi = {10.1016/j.compfluid.2016.05.033},
file = {:C$\backslash$:/Library/Papers/Nozzles/Papers/2016 - Cetin et al. - Computational analysis of nozzle geometry variations for subsonic turbulent jets.pdf:pdf},
issn = {00457930},
journal = {Comput. Fluids},
keywords = {Cartesian mesh method,Large-eddy simulation,Multi-shear-layer flow,Nozzle built-in components,Turbulent jet},
pages = {467--484},
title = {{Computational analysis of nozzle geometry variations for subsonic turbulent jets}},
volume = {136},
year = {2016}
}
@article{Singh2009,
abstract = {Experiments were conducted using tufts and PIV to determine the conditions for which a swirled gas jet issuing from a sharp-edge nozzle, in flush with a base plate, would form a Coanda jet. The flow field was also simulated. The inception of the Coanda jet was observed to be associated with the formation of a recirculation bubble at the nozzle exit. A threshold value of swirl number, which increased monotonically with Reynolds number, was required for the formation of the Coanda jet. The Coanda jet was associated with hysteresis. The flow features and transition from a diverging jet to a Coanda jet are discussed. {\textcopyright} 2009 Elsevier Inc.},
