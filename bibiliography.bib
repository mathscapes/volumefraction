doi = {10.1080/00405000.2014.906941},
file = {:C$\backslash$:/Library/Papers/Impact Study/2017-18/2015 - Pan, Gu, Sun - Numerical analyses of thermo-mechanical behaviors of 3-D rectangular braided composite under different temperature.pdf:pdf},
isbn = {0040-5000},
