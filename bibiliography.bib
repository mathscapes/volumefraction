doi = {10.1016/j.compositesb.2007.02.020},
file = {:C$\backslash$:/Library/Papers/Impact Study/Papers on Blast Loading/2008 - Tekalur, Shivakumar, Shukla - Mechanical behavior and damage evolution in E-glass vinyl ester and carbon composites subjected to.pdf:pdf},
