doi = {10.1007/s10409-006-0015-8},
file = {:C$\backslash$:/Library/Papers/Impact Study/2017-18/2006 - Chen, Li, Fan - Oblique perforation of thick metallic plates by rigid projectiles.pdf:pdf},
isbn = {0567-7718},
