doi = {10.1016/j.compositesb.2016.03.054},
file = {:C$\backslash$:/Library/Papers/Impact Study/2017-18/2016 - Sun et al. - On compressive properties of composite sandwich structures with grid reinforced honeycomb core.pdf:pdf},
