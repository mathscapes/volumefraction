pages = {464--469},
title = {{A Numerical Investigation of the Performance of a Nacre-Like Composite under Blast Loading}},
url = {https://www.engineeringvillage.com/share/document.url?mid=inspec{\_}5a94fbf11579b8a6332M61a410178163171{\&}database=ins},
