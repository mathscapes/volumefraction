@article{Mamrak2011,
abstract = {Analiza propagacji ci{\'{s}}nienia w powietrzu i obci{\c{a}}{\.{z}}enia wybuchem sztywnej przeszkody Obci{\c{a}}{\.{z}}enia wyj{\c{a}}tkowe odgrywaj{\c{a}} kluczow{\c{a}} rol{\c{e}} w projektowaniu wielu obiekt{\'{o}}w, w szczeg{\'{o}}lno{\'{s}}ci obiekt{\'{o}}w u{\.{z}}yteczno{\'{s}}ci publicznej. Na przestrzeni ostatnich lat uwzgl{\c{e}}dnienie wp{\l}ywu obci{\c{a}}{\.{z}}enia wybuchem sta{\l}o si{\c{e}} nierzadko wymogiem w krajach Europy Zachodniej ze wzgl{\c{e}}du na rosn{\c{a}}ce i coraz cz{\c{e}}stsze gro{\'{z}}by atak{\'{o}}w terrorystycznych. W obliczu tak powa{\.{z}}nych wyzwa{\'{n}} projektanci wci{\c{a}}{\.{z}} nie zdo{\l}ali doczeka{\'{c}} si{\c{e}} norm prawnych opisuj{\c{a}}cych proces projektowania obiekt{\'{o}}w zdolnych przeciswstawi{\'{c}} si{\c{e}}, w mo{\.{z}}liwym stopniu, wspomnianym zagro{\.{z}}eniom. Ta sytuacja, wesp{\'{o}}{\l} z obecn{\c{a}} tendencj{\c{a}} do wznoszenia konstrukcji lekkich i delikatnych, w zwi{\c{a}}zku z czym tak{\.{z}}e bardziej podantych na obci{\c{a}}{\.{z}}enia dynamiczne i lokalne zniszczenia, dodatkowo pot{\c{e}}guje zagro{\.{z}}enia gro{\.{z}}{\c{a}}ce u{\.{z}}ytkownikom tych{\.{z}}e obiekt{\'{o}}w. Niniejsza praca stanowi pr{\'{o}}b{\c{e}} zmierzenia si{\c{e}} z wyzwaniami oceny si{\l}y fali uderzeniowej powsta{\l}ej w skutek wybuchu jak i z{\l}o{\.{z}}onymi zjawiskami wp{\l}ywaj{\c{a}}cymi na jej propagacj{\c{e}}. W pracy om{\'{o}}wiono rodzaje obci{\c{a}}{\.{z}}e{\'{n}} wyj{\c{a}}tkowych ze szczeg{\'{o}}lnym naciskiem na obci{\c{a}}{\.{z}}enie wybuchem. Przedstawiono definicj{\c{e}} i klasyfikacj{\c{e}} materia{\l}{\'{o}}w wybuchowych oraz precyzyjnie om{\'{o}}wiono proces propagacji fali uderzeniowej w powietrzu. Dodatkowo, zaprezentowano aktualny stan prawny i regulacje, zar{\'{o}}wno europejskie jak i ameryka{\'{n}}skie, dotycz{\c{a}}ce projektowania konstrukcji nara{\.{z}}onych na obci{\c{a}}{\.{z}}enia wybuchem. Przywo{\l}ano prace badawcze z ca{\l}ego {\'{s}}wiata dotycz{\c{a}}ce rozpatrywanych zagadnie{\'{n}}. W celu wykonania oblicze{\'{n}} numerycznych pos{\l}u{\.{z}}ono si{\c{e}} programem komputerowym Abaqus. Do tworzenia modeli wykorzystano j{\c{e}}zyk programowania Python wraz z Interfejsem Programowania Aplikacji programu Abaqus dla tego{\.{z}} j{\c{e}}zyka. Przeprowadzono zesp{\'{o}}{\l} analiz dotycz{\c{a}}cych trzech g{\l}{\'{o}}wnych zagadnie{\'{n}}: -okre{\'{s}}lenia odpowiedniego rozmiaru elementu sko{\'{n}}czonego do dyskretyzacji modeli, okre{\'{s}}lenia energii w{\l}a{\'{s}}ciwej trotylu do wykorzystania w p{\'{o}}{\'{z}}niejszych analizach oraz okre{\'{s}}lenia zgodno{\'{s}}ci wynik{\'{o}}w dla prostego modelu, -weryfikacji ci{\'{s}}nie{\'{n}} wewn{\c{e}}trznych powsta{\l}ych w wyniku eksplozji zewn{\c{e}}trznej, -wp{\l}ywu zjawiska os{\l}aniania na warto{\'{s}}ci ci{\'{s}}nie{\'{n}} i impuls{\'{o}}w wraz z por{\'{o}}wnaniem dw{\'{o}}ch program{\'{o}}w komputerowych wykorzystuj{\c{a}}cych odmienne metody numeryczne i weryfikacj{\c{a}} koncepcji odleg{\l}o{\'{s}}ci skalowanej.},
author = {Mamrak, Wojciech and Wierszycki, Marcin and Sielicki, Piotr},
file = {:C$\backslash$:/Library/Papers/Impact Study/Papers on Blast Loading/Unknown - Mamrak - Blast wave propagation in the air and action on rigid obstacles.pdf:pdf},
pages = {106},
title = {{Blast wave propagation in the air and action on rigid obstacles}},
