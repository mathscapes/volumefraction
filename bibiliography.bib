file = {:C$\backslash$:/Library/Papers/Impact Study/2017-18/2015 - Mars et al. - Finite element implementation of an orthotropic plasticity model for sheet metal in low velocity impact simulations.pdf:pdf},
issn = {02638231},
journal = {Thin-Walled Struct.},
keywords = {Aluminum,Anisotropy,Elasto-plastic model,FE modeling,Low velocity impact},
