journal = {Compos. Struct.},
keywords = {Cohesive elements,Delamination,Finite element analysis (FEA),Impact behaviour},
number = {1},
pages = {161--171},
publisher = {Elsevier Ltd},
title = {{Finite element modelling of damage induced by low-velocity impact on composite laminates}},
url = {http://dx.doi.org/10.1016/j.compstruct.2013.09.004},
volume = {108},
year = {2014}
}
