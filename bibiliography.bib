doi = {10.1016/j.matdes.2008.04.048},
file = {:C$\backslash$:/Library/Papers/Impact Study/2017-18/2009 - Rusinek, Klepaczko - Experiments on heat generated during plastic deformation and stored energy for TRIP steels.pdf:pdf},
isbn = {0261-3069},
issn = {02641275},
journal = {Mater. Des.},
keywords = {Plastic work,Stored energy,TRIP steel,Temperature measurement,Thermal coupling},
number = {1},
pages = {35--48},
title = {{Experiments on heat generated during plastic deformation and stored energy for TRIP steels}},
volume = {30},
year = {2009}
}
