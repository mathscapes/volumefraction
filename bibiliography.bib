}
@article{Li2017,
abstract = {Cylinder casing filled with explosive under asymmetric initiation is an easy way to increase the warhead efficiency. The radial distribution of fragment velocity is an important design parameter for the asymmetrically initiated warhead. Available formulas either can only apply to calculating fragment velocity of certain radial direction, or their applications need experiments to determine unknown parameters. Establishment of a new math model to calculate velocity distribution was presented in this paper. Firstly, in the central section of warhead cylinder (treated as 2D problem), a stationary point was proposed existing between the asymmetric initiation point and the warhead center. From this stationary point the warhead can be radially cut into many segments of explosive and fragments. Then the ratios of charge mass to casing mass of those segments were computed, corrected and used to obtain fragments velocities by Gurney formula. Most importantly, the position of the stationary point was theoretically determined by the one-dimensional detonation pushing piston theory (Zhang etal., 2001) [14]. The math model established in the present work was finally validated with the numerical modeling and the experiment results. The results indicate that the model can accurately compute the radial distribution of fragment velocity of asymmetrically initiated warhead and is completely theoretically enclosed, needing no experiments to determine unknown parameters. In addition, the fragment velocity distribution gotten from experiments was found to be different with the theoretical velocity distribution near the initiation side because of influence of the initiator volume.},
author = {Li, Yuan and hua Li, Yan and quan Wen, Yu},
doi = {10.1016/j.ijimpeng.2016.09.007},
file = {:C$\backslash$:/Library/Papers/Impact Study/2017-18/2017 - Li, Li, Wen - Radial distribution of fragment velocity of asymmetrically initiated warhead.pdf:pdf},
issn = {0734743X},
journal = {Int. J. Impact Eng.},
keywords = {Aimable warhead,Asymmetric initiation,Fragment velocity,Gurney formula},
pages = {39--47},
publisher = {Elsevier Ltd},
title = {{Radial distribution of fragment velocity of asymmetrically initiated warhead}},
url = {http://dx.doi.org/10.1016/j.ijimpeng.2016.09.007},
volume = {99},
year = {2017}
}
@book{Lee2010,
abstract = {There is considerable demand for a reliable computational methodology capable of being used in design studies of composite structures to determine the level of blast protection. As an example, a summary of a comprehensive methodology developed at the University of Maryland for the assessment of damage to composite structures under high explosive blast pressure loading is provided. A simplified methodology is then introduced to obtain structural responses at various blast wave pressure loadings and predict structural collapse based on these responses. The results of numerical tests on flat laminated composite panels show that this simplified methodology, which does not require computationally expensive failure analysis including the progressive material damages, can predict structural failure with reasonable accuracy. Accordingly, the methodology could be used as a first screening tool for design study, and then a full analysis including material damage and failure could be conducted in order to provide detailed predictions of local damage to the selected designs. {\textcopyright} 2010 Woodhead Publishing Limited. All rights reserved.},
