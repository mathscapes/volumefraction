keywords = {Aerospace materials,Ballistic impact,Metals and alloys,Thin aluminium plate,Uniaxial preload},
number = {5},
pages = {404--407},
title = {{Behaviour of uniaxially preloaded aluminium plates subjected to high-velocity impact}},
volume = {38},
year = {2011}
}
@article{Shamsuddoha2017,
abstract = {{\textcopyright} 2016, {\textcopyright} The Author(s) 2016. Fiber-reinforced composites are a well-recognized option for repair and rehabilitation of the pipelines for the oil and gas industry. Infilled composite sleeve system provides an effective rehabilitation solution, where the sleeve acts as prime reinforcement without any direct contact with steel. However, the long-term performance of the repair is dependent, in part, on the effect of hygrothermal ageing of the composites. In this publication, glass transition temperature and mechanical properties are compared for glass-fiber reinforced vinyl ester composite, both as-manufactured and after hot-wet conditioning at 80℃. The tensile and shear strength reduced substantially during conditioning, whilst the elastic modulus was relatively stable. The average glass transition temperature of the composite dropped from the as-manufactured value of 110℃ to 97℃ and 101℃, after 1000 and 3000 h of conditioning, respectively, indicating that it is stable and that the composite is suitable for use as a pipeline repair material operating at 80℃. The results indicate that a 1000 h conditioning period, specified as a minimum period in ISO/TS 24817 is suitable for representing long-term properties for stiffness-based designs for the composite material and conditioning temperature investigated.},
author = {Shamsuddoha, Md and Djukic, Luke P. and Islam, Md Mainul and Aravinthan, Thiru and Manalo, Allan},
doi = {10.1177/0021998316661869},
file = {:C$\backslash$:/Library/Papers/Impact Study/2017-18/2017 - Shamsuddoha et al. - Mechanical and thermal properties of glass fiber–vinyl ester resin composite for pipeline repair exposed to.pdf:pdf},
issn = {1530793X},
journal = {J. Compos. Mater.},
keywords = {Glass fibers,glass transition temperature,mechanical properties,pipeline,polymer-matrix composite,thermal properties,vinyl ester resin},
number = {11},
pages = {1605--1617},
title = {{Mechanical and thermal properties of glass fiber–vinyl ester resin composite for pipeline repair exposed to hot-wet conditioning}},
volume = {51},
year = {2017}
}
